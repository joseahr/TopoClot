<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.topobras.topobras"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/topoclot"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CurvaCircular"
            android:label="@string/title_activity_curva_circular"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Radiar"
            android:label="@string/title_activity_radiar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Clotoide"
            android:label="@string/title_activity_clotoide"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".DatosCampo"
            android:label="@string/title_activity_datos_campo"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ActivityListaPuntos"
            android:label="@string/title_activity_activity_lista_puntos" >
        </activity>
    </application>

</manifest>
